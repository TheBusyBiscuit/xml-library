{
  "name": "xml-library",
  "version": "0.2.0",
  "description": "A library that can perform XML > JSON and JSON > XML parsing (Now with Promises!)",
  "main": "lib/XML.js",
  "scripts": {
    "test": "mocha test/*.js --reporter mocha-multi-reporters --reporter-options configFile=mocha.json",
    "code-coverage": "nyc --reporter=lcov --reporter=text-lcov mocha test/*.js",
    "sonar": "node ./lib/sonar.js"
  },
  "keywords": [
    "xml",
    "json",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TheBusyBiscuit/xml-library"
  },
  "author": "TheBusyBiscuit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheBusyBiscuit/xml-library/issues"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "child-process-promise": "2.2.1",
    "mocha": "9.2.0",
    "mocha-multi-reporters": "1.5.1",
    "mocha-sonarqube-reporter": "1.0.2",
    "nyc": "15.1.0"
  }
}
